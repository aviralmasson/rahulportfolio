<style>


    .container {
        display: flex;
        justify-content: space-evenly;
        align-items: center;
        height: 100vh;
    }

    .box {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        width: 30%;
        height: 270px;
        border-radius: 15px;
        transition: .5s;
        box-shadow: 0 10px 10px rgba(0, 0, 0, 0.2);
    }
    .box--color {
        background: linear-gradient(to right,  #FBB54E 0%, #FF0040 100%);
    }
    .box--orange {
        background: linear-gradient(to right, #FBB54E 0%, #FF9900 100%);
    }
    .box--green {
        background: linear-gradient(to right, #76EED6 0%, #00D5AA 100%);
    }
    .box .profile {
        margin-top: 10px;
        width: 110px;
        height: 110px;
        background-color:#f7f7f7 ;
        border-radius: 50%;
    }

    .box .profile img {

        margin-top: 15px;


    }
    .box .content {
        display: flex;
        flex-direction: column;
        align-items: center;
        text-align: center;
        padding: 30px;

    }
    .box .content h1 {
        font-weight: 600;
        font-size: 28px;
        color: black;
        margin: 10px;

    }
    .box .content p {
        color: black;
        font-size: 15px;
    }
    .box:hover {
        -webkit-transform: scale(1.1);
        transform: scale(1.1);
    }

    @media (max-width: 700px) {
        .container {
            flex-direction: column;
        }
        .container .box {
            margin: 5px;
            padding: 5px;
        }
        .container .box .profile {
            margin-top: 10px;
            width: 60px;
            height: 120px;
        }
        .container .box .content {
            text-align: center;
        }
        .container .box .content h1 {
            margin-top: -20px;
            font-size: 24px;
        }
        .container .box .content p {
            font-size: 14px;
        }
    }


    h1 {
        font-family: "Avant Garde", Avantgarde, "Century Gothic", CenturyGothic, "AppleGothic", sans-serif;
        font-size: 92px;
        padding: 80px 50px;
        text-align: center;
        text-transform: uppercase;
        text-rendering: optimizeLegibility;
